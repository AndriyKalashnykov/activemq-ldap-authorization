ldap-with-roles {
    org.eclipse.jetty.jaas.spi.LdapLoginModule required
    debug="false"
    useLdaps="false"
    contextFactory="com.sun.jndi.ldap.LdapCtxFactory"
    hostname=""
    port="389"
    bindDn="CN=,OU=Service-Accounts,DC=my,DC=domain,DC=com"
    bindPassword="##########"
    authenticationMethod="simple"
    forceBindingLogin="false"
    userBaseDn="ou=Users,DC=my,DC=domain,DC=com"
    userRdnAttribute="sAMAccountName"
    userIdAttribute="sAMAccountName"
    userPasswordAttribute="unicodePwd"
    userObjectClass="user"
    userRoleName="memberof"
    authzIdentity="{displayName}"
    roleSearch="(member={0})"
    roleName="cn"
    roleSubtree="true"
    roleBaseDn="ou=Keybox-Groups,DC=my,DC=domain,DC=com"
    roleNameAttribute="sAMAccountName"
    roleMemberAttribute="member"
    roleObjectClass="group"
};

LDAPLogin-Jetty {
  org.eclipse.jetty.plus.jaas.spi.LdapLoginModule required
    debug="true"
    contextFactory="com.sun.jndi.ldap.LdapCtxFactory"
    hostname="##### LDAP_HOST #####"
    port="##### LDAP_PORT #####"
    bindDn="##### LDAP_CONN_USER #####"
    bindPassword="##### LDAP_CONN_USER_PWD #####"
    authenticationMethod="simple"
    forceBindingLogin="true"
    userBaseDn="##### LDAP_USER_BASE #####"
    userRdnAttribute="uid"
    userIdAttribute="uid"
    userPasswordAttribute="userPassword"
    userObjectClass="inetOrgPerson"
    roleBaseDn="ou=Group,ou=ActiveMQ,ou=system"
    roleNameAttribute="cn"
    roleMemberAttribute="member"
    roleObjectClass="groupOfNames"
    authenticated="true";
};